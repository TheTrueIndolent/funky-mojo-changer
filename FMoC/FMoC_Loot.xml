<!-- Loot Button  -->
	<Button name="fmocButtonLoot" inherits="InsecureActionButtonTemplate" frameStrata="BACKGROUND" frameLevel="0" movable="true" dontSavePosition="true" clampedToScreen="true" virtual="true">
