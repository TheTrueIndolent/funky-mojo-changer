						<Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="0"/>
						<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="BORDER">
				<Texture atlas="talents-node-choiceflyout-square-yellow" desaturated="true">
					<Anchors>
						<Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="0"/>
						<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Button>
	<Button name="fmocButtonLoot1" parent="UIParent" inherits="fmocButtonLoot"/>
	<Button name="fmocButtonLoot2" parent="UIParent" inherits="fmocButtonLoot"/>
	<Button name="fmocButtonLoot3" parent="UIParent" inherits="fmocButtonLoot"/>
